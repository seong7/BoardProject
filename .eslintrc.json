{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-empty": 0,
    "no-console": "warn",
    "import/prefer-dafualt-export": 0,
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [0, "error", "windows"],
    "no-alert": "warn",
    "no-unused-vars": "warn",
    "quotes": [2, "double", { "avoidEscape": false }],
    "no-plusplus": "off"
  }
}
